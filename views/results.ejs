<h1>hello</h1>

 <style>
 div {
     float:left;
     
      
 }
 img {
     width: 300px;
     height: 300px;
     margin:3px;
 }
 </style>
<h1>Search for Movie</h1>

<form action="/result" method="GET">
    <input type="text" name="search">
    <input type="submit">
</form>
<% data.Search.forEach(function(movie){ %>
    <div>
    <img src="<%= movie.Poster %>" >
    <h3> <%= movie.Title %> - <%= movie.Year %> </h3>
    </div>
   
<% }) %>

